    "@types/mdast": "^4.0.4",
    "rollup": "^4.24.4"
  },
  "dependencies": {
    "@dolphin/common": "workspace:^",
    "js-base64": "^3.7.7",
    "lodash-es": "^4.17.21",
    "mdast-util-gfm-strikethrough": "^2.0.0",
    "mdast-util-gfm-table": "^2.0.0",
    "mdast-util-gfm-task-list-item": "^2.0.0",
    "mdast-util-math": "^3.0.0",
    "mdast-util-to-markdown": "^2.1.2"
  }
