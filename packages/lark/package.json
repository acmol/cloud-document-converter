{
  "name": "@dolphin/lark",
  "version": "2.5.4",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./docx": {
      "types": "./dist/docx.d.ts",
      "default": "./dist/docx.js"
    },
    "./env": {
      "types": "./dist/env.d.ts",
      "default": "./dist/env.js"
    },
    "./image": {
      "types": "./dist/image.d.ts",
      "default": "./dist/image.js"
    }
